# Frontend Environment Configuration
# Copy this to .env.local and update with your values

# Storage Keys (Change these in production)
REACT_APP_STORAGE_KEY=partyoria_events_prod
REACT_APP_DRAFT_KEY=partyoria_draft_prod

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_TIMEOUT=10000

# Security Configuration
REACT_APP_ENABLE_LOGGING=false
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_VENDOR_PORTAL=true
REACT_APP_ENABLE_ANALYTICS=false